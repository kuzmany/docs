{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mautic Extendee",
  "icons": {
    "library": "fontawesome"
  },
  "colors": {
    "primary": "#2563EB",
    "light": "#60A5FA",
    "dark": "#1D4ED8"
  },
  "typography": {
    "font": "Inter",
    "headings": "Montserrat"
  },
  "modeToggle": {
    "default": "dark"
  },
  "favicon": "/favicon.svg",
  "styling": {
    "eyebrows": "breadcrumbs",
    "headings": {
      "fontSize": {
        "h1": "2.5rem",
        "h2": "2rem",
        "h3": "1.5rem"
      },
      "fontWeight": {
        "h1": "800",
        "h2": "700",
        "h3": "600"
      },
      "letterSpacing": {
        "h1": "-0.02em"
      }
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "pages": [
          "homepage"
        ]
      },
      {
        "tab": "Twig Templates",
        "pages": [
          "twig-templates/introduction",
          "twig-templates/how-to-install",
          "twig-templates/how-to-use",
          "twig-templates/available-tokens",
          "twig-templates/snippets",
          "twig-templates/custom-objects-support",
          "twig-templates/forms-support",
          "twig-templates/embedding-externally",
          "twig-templates/api"
        ]
      },
      {
        "tab": "Changelog",
        "pages": [
          "changelog"
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/logo.png",
    "dark": "/logo/logo.webp"
  },
  "navbar": {
    "links": [
      {
        "name": "Website",
        "url": "https://www.mtcextendee.com/"
      }
    ]
  },
  "footer": {
    "socials": {
    }
  }
}

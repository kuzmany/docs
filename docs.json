{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",

  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "typography": {
    "font": "Inter",
    "headings": "Montserrat"
  },
  "modeToggle": {
    "default": "dark"
  },
  "favicon": "/favicon.svg",
  "styling": {
    "eyebrows": "breadcrumbs",
    "headings": {
      "fontSize": {
        "h1": "2.5rem",
        "h2": "2rem",
        "h3": "1.5rem"
      },
      "fontWeight": {
        "h1": "800",
        "h2": "700",
        "h3": "600"
      },
      "letterSpacing": {
        "h1": "-0.02em"
      }
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "pages": [
          "index"
        ]
      },
      {
        "tab": "Twig Templates",
        "pages": [
          "twig-templates/introduction",
          "twig-templates/how-to-install",
          "twig-templates/how-to-use"
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
    ]
  },
  "footer": {
    "socials": {
    }
  }
}
